<tool id="gx_data_multiple" name="gx_data_multiple" version="1.0.0">
    <command><![CDATA[
cat '$parameter' >> '$output'
    ]]></command>
    <inputs>
        <param name="parameter" type="data" ext="data" multiple="true" />
    </inputs>
    <outputs>
        <data name="output" format="data" />
    </outputs>
    <tests>
        <test>
            <param name="parameter" value="12456" />
            <output name="output">
                <assert_contents>
                    <has_line line="12456" />
                </assert_contents>
            </output>
        </test>
    </tests>
</tool>
